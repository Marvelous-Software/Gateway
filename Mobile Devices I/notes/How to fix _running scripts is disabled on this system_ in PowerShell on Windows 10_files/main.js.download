                        _fetch_user_isp( response );
                    }
                },
                'json'
            );
        };



        var _maybe_show_popup = function() {
            var show_popup = false;
            // Check if popup cookie is set. Show popup if not.
            var popup_cookie_set_date = parseInt( ADDTIPS.GENERAL().get_cookie( _POPUP_COOKIE_NAME + _popup_id ) );
            // Extract from the cookie value the date when it was last dismissed. Always show when previewing popup.
            if ( _$popup.closest( '.addtips_popup_box__preview' ).length === 1 ) {
                show_popup = true;
            } else {
                if ( ! popup_cookie_set_date ) {
                    show_popup = true;
                } else {
                    // Fetch the NOW timestamp in milliseconds.
                    var today = new Date().getTime();
                    // Compute the number of days passed since the popup was last dismissed.
                    var num_days_passed = ( today - popup_cookie_set_date ) / ADDTIPS.GENERAL().DAY_IN_MILLISECONDS;
                    // Check if the max num of days has been exceeded.
                    if ( num_days_passed >= _popup_num_hide_days ) {
                        show_popup = true;
                        // Force unset the cookie.
                        ADDTIPS.GENERAL().set_cookie( _POPUP_COOKIE_NAME + _popup_id, '', - _popup_num_hide_days );
                    }
                }
            }
            if ( show_popup ) {
