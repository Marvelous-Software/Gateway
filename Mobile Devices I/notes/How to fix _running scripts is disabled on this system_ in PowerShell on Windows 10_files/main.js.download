    ADDTIPS.Handle_Repeating_Sidebar_Widget = function() {

        var self = this;
        var _wrapper_classes = null;
        var _$sidebar = null;
        var _$widget = null;



        self.init = function( sidebar_css_class, wrapper_classes ) {
            _$sidebar = $( '.' + sidebar_css_class );
            _wrapper_classes = wrapper_classes;
            // Create a clone of the widget.
            _$widget = $( '.repeating-widget-wrapper', _$sidebar ).children( '.repeating-widget' ).clone();
            // Mark the initial widget wrapper as populated with a widget.
            $( '.repeating-widget-wrapper', _$sidebar ).attr( 'data-widget-loaded', 'yes' );

            $( window ).load( function() {
                // Replicate the widget wrappers and set their height.
                var sidebar_height = _$sidebar.height();
                var window_height = $( window ).height();
                var container_height;
                if ( _$sidebar.height() > window_height * 2 ) {
                    container_height = window_height * 2;
                } else {
                    container_height = _$sidebar.height();
                }
                var total_wrappers_height = container_height;
                $( '.repeating-widget-wrapper', _$sidebar ).height( container_height );
                while ( total_wrappers_height < sidebar_height  ) {
                    _$sidebar.append( '<div class="' + _wrapper_classes + '"></div>' );
                    if ( sidebar_height > total_wrappers_height + window_height * 2 ) {
                        container_height = window_height * 2;
                    } else {
                        container_height = sidebar_height - total_wrappers_height;
                    }
                    total_wrappers_height += container_height;
                    $( '.repeating-widget-wrapper', _$sidebar ).last().height( container_height );
                }

                // As the user scrolls, check all wrappers if they are approaching the viewport, to load the widget.
                $( window ).scroll( function() {
                    $( '.repeating-widget-wrapper:not([data-widget-loaded])', _$sidebar ).each( function() {
                        if ( typeof $( this )[0].getBoundingClientRect === 'function' ) {
                            // If the wrapper is approaching the viewport, insert the widget.
                            if ( $( this )[0].getBoundingClientRect().top <= window_height + 20 ) {
                                $( this ).append( _$widget.clone() ).attr( 'data-widget-loaded', 'yes' );
                            }
                        }
                    });
                });
            });

        };

    };



