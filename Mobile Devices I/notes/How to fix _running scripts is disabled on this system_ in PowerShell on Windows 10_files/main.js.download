                    if ( _$popup.closest( '.addtips_popup_box__preview' ).length === 0 ) {
                        ADDTIPS.GENERAL().set_cookie( _POPUP_COOKIE_NAME + _popup_id, new Date().getTime(), _popup_num_hide_days );
