    ADDTIPS.Handle_Affiliate_Links = function() {

        var self = this;
        var _this_page_path;



        /**
         * Update the value of a data parameter in a URL.
         *
         * @param string The original URL
         * @param integer The position at which the parameter is, starting at 0
         * @param string The value to set for the parameter
         * @return string The updated URL
         */
        var _update_data_parameter = function( href, param_index, param_value, update_if_not_empty ) {
            var data_params = 'd/';
            if ( href.indexOf( '/d/' ) !== -1 ) {
                // Extract the data parameters component of the affiliate link, using regular expression.
                var current_data_params = href.match( /\/d\/[^\/]+\/?/ )[0];
                // Remove the data params from the href.
                href = href.replace( current_data_params, '' );
                // Extract just the data params, as an array
                current_data_params = current_data_params.replace( '/d/', '' ).replace( '/', '' ).split( '+' );
                // Enforce the param value at the required position, if allowed and/or not populated already.
                if ( update_if_not_empty ||
                        ( typeof current_data_params[ param_index ] === 'undefined' ) ) {
                    current_data_params[ param_index ] = param_value;
                }
                // Glue the data parameters back together.
                data_params += current_data_params.join( '+' );
            } else {
                // No data parameters in the affiliate link, at the moment.
                // Add as many glue chars as needed, to reach the index.
                for ( var counter = 0; counter < param_index; counter ++ ) {
                    data_params += '+';    
                }
                data_params += param_value;
            }

            // Now append the data parameters to the affiliate link, minding the trailing slash.
            if ( href.slice( -1 ) !== '/' ) {
                href += '/';
            }
            href += data_params + '/';

            return href;
        };



