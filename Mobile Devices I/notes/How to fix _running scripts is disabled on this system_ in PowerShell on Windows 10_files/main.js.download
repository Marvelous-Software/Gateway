                    }
                },
                'json'
            );
        };



        var _fetch_user_ip_address = function() {
            $.post(
                addtips.ajaxurl + '?action=addtips_popup__get_user_ip_address',
                {},
                function( response ) {
                    if ( ( typeof response !== 'undefined' ) && ( response !== '' ) ) {
                        var $ip_address_field = $( '.addtips_user_ip_address', _$popup );
                        $( 'em', $ip_address_field ).text( response );
                        $ip_address_field.show();
